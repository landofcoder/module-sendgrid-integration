<?xml version="1.0" ?>
<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
	<argument name="data" xsi:type="array">
		<item name="js_config" xsi:type="array">
			<item name="provider" xsi:type="string">lof_sendgrid_singlesend_listing.lof_sendgrid_singlesend_listing_data_source</item>
		</item>
	</argument>
	<settings>
		<spinner>lof_sendgrid_singlesend_columns</spinner>
		<deps>
			<dep>lof_sendgrid_singlesend_listing.lof_sendgrid_singlesend_listing_data_source</dep>
		</deps>
	</settings>
	<dataSource component="Magento_Ui/js/grid/provider" name="lof_sendgrid_singlesend_listing_data_source">
		<settings>
			<updateUrl path="mui/index/render"/>
		</settings>
		<aclResource>Lof_SendGrid::SingleSend</aclResource>
		<dataProvider class="Magento\Framework\View\Element\UiComponent\DataProvider\DataProvider" name="lof_sendgrid_singlesend_listing_data_source">
			<settings>
				<requestFieldName>id</requestFieldName>
				<primaryFieldName>singlesend_id</primaryFieldName>
			</settings>
		</dataProvider>
	</dataSource>
	<listingToolbar name="listing_top">
		<settings>
			<sticky>true</sticky>
		</settings>
		<bookmark name="bookmarks"/>
		<columnsControls name="columns_controls"/>
		<filters name="listing_filters"/>
		<paging name="listing_paging"/>
	</listingToolbar>
	<columns name="lof_sendgrid_singlesend_columns">
		<selectionsColumn name="ids">
			<settings>
				<indexField>entity_id</indexField>
			</settings>
		</selectionsColumn>
		<column name="entity_id">
			<settings>
				<filter>text</filter>
				<sorting>asc</sorting>
				<label translate="true">ID</label>
			</settings>
		</column>
		<column name="singlesend_id">
			<settings>
				<filter>text</filter>
				<label translate="true">Single Send Id</label>
			</settings>
		</column>
		<column name="name">
			<settings>
				<filter>text</filter>
				<label translate="true">name</label>
			</settings>
		</column>
        <column name="status">
            <settings>
                <filter>text</filter>
                <label translate="true">Status</label>
            </settings>
        </column>
		<column name="create_date">
			<settings>
				<filter>text</filter>
				<label translate="true">create_date</label>
			</settings>
		</column>
		<column name="update_date">
			<settings>
				<filter>text</filter>
				<label translate="true">update_date</label>
			</settings>
		</column>
	</columns>
</listing>
