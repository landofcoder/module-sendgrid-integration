<?xml version="1.0" ?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
	<acl>
		<resources>
			<resource id="Magento_Backend::admin">
				<resource id="Magento_Backend::stores">
					<resource id="Magento_Backend::stores_settings">
						<resource id="Magento_Config::config">
							<resource id="Lof_SendGrid::config_lof_sendgrid" title="sendgrid"/>
						</resource>
					</resource>
				</resource>
				<resource id="Lof_SendGrid::Campaigns" sortOrder="10" title="Campaigns">
					<resource id="Lof_SendGrid::Campaigns_delete" sortOrder="20" title="Delete Campaigns"/>
					<resource id="Lof_SendGrid::Campaigns_view" sortOrder="40" title="View Campaigns"/>
				</resource>
				<resource id="Lof_SendGrid::Statistics" sortOrder="15" title="Statistics"></resource>
			</resource>
		</resources>
	</acl>
</config>
